{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "core:window:allow-minimize",
    "core:window:allow-hide",
    "core:window:allow-close",
    "core:window:allow-set-focus",
    "core:window:allow-is-visible",
    "core:window:allow-show",
    "core:window:allow-create",
    "core:window:allow-set-position",
    "core:window:allow-destroy",
    "core:webview:allow-create-webview-window",
    "core:webview:allow-set-webview-size",
    "core:webview:allow-set-webview-position",
    "core:webview:allow-create-webview",
    "core:webview:allow-webview-show",
    "core:webview:allow-webview-hide",
    "core:webview:allow-webview-close",
    "window-state:default",
    "window-state:allow-save-window-state",
    "window-state:allow-restore-state",
    "fs:default",
    "fs:read-all",
    "fs:read-files",
    "fs:write-all",
    "fs:write-files",
    "fs:scope-log",
    "fs:allow-exists",
    "fs:allow-open",
    "fs:allow-read-dir",
    "fs:allow-resource-read",
    "fs:allow-resource-read-recursive",
    "fs:allow-resource-write",
    "fs:allow-resource-write-recursive",
    "fs:allow-app-read-recursive",
    "log:default",
    "log:allow-log",
    "os:default",
    "core:tray:default",
    "core:tray:allow-new",
    "core:tray:allow-remove-by-id",
    "core:tray:allow-set-icon",
    "core:tray:allow-set-icon-as-template",
    "core:tray:allow-set-menu",
    "core:tray:allow-set-show-menu-on-left-click",
    "core:tray:allow-set-title",
    "core:tray:allow-set-tooltip",
    "core:tray:allow-set-visible",
    "core:window:allow-unminimize",
    "core:window:allow-is-minimized",
    "core:app:allow-default-window-icon",
    "core:path:allow-is-absolute",
    "notification:default",
    "shell:default",
    "shell:allow-execute",
    "shell:allow-kill",
    "shell:allow-open",
    "shell:allow-spawn",
    "shell:allow-stdin-write",
    {
      "identifier": "fs:write-all",
      "allow": [
        {
          "path": "$RESOURCE/*"
        },
        {
          "path": "$RESOURCE/config/*"
        }
      ]
    },
    {
      "identifier": "fs:read-all",
      "allow": [
        {
          "path": "$RESOURCE/**"
        },
        {
          "path": "$RESOURCE/config/*"
        }
      ]
    },
    {
      "identifier": "shell:allow-execute",
      "allow": [
        {
          "args": true,
          "cmd": "$RESOURCE/resource/nssm",
          "name": "nssm"
        },
        {
          "args": true,
          "cmd": "$RESOURCE/resource/easytier-core",
          "name": "easytier-core"
        },
        {
          "args": true,
          "cmd": "$RESOURCE/resource/easytier-cli",
          "name": "easytier-cli"
        },
        {
          "args": true,
          "cmd": "explorer",
          "name": "explorer"
        },
        {
          "args": true,
          "cmd": "netsh",
          "name": "netsh"
        },
        {
          "args": true,
          "cmd": "taskkill",
          "name": "taskkill"
        },
        {
          "args": true,
          "cmd": "killall",
          "name": "killall"
        },
        {
          "args": true,
          "cmd": "kill",
          "name": "kill"
        },
        {
          "name": "exec-sh",
          "cmd": "sh",
          "args": [
            "-c",
            {
              "validator": "\\S+"
            }
          ],
          "sidecar": false
        },
        {
          "name": "any",
          "cmd": "**",
          "args": true,
          "sidecar": false
        }
      ]
    },
    {
      "identifier": "shell:allow-spawn",
      "allow": [
        {
          "args": true,
          "cmd": "$RESOURCE/resource/easytier-core",
          "name": "easytier-core"
        },
        {
          "args": true,
          "cmd": "$RESOURCE/resource/easytier-cli",
          "name": "easytier-cli"
        }
      ]
    }
  ]
}