{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "identifier": "easytier-manager-pro",
  "bundle": {
    "windows": {
      "allowDowngrades": true,
      "webviewInstallMode": {
        "type": "embedBootstrapper",
        "silent": true
      },
      "nsis": {
        "displayLanguageSelector": true,
        "installMode": "perMachine",
        "languages": [
          "SimpChinese",
          "English"
        ]
      },
      "wix": {
        "language": [
          "zh-CN"
        ]
      }
    }
  }
}